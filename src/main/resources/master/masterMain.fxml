<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.musicapp.gui.MasterMainController"
      prefHeight="400.0" prefWidth="600.0">

    <Label fx:id="headline" text="Master"/>

    <Label fx:id="maxCon" text="Maximale Anzahl an Clients"/>

    <TextField fx:id="setMaxConnections" > </TextField>

    <Button fx:id="connect" text="Back" onAction="#connectToClients"/>

    <Button fx:id="back" text="Back" onAction="#back"/>

</VBox>
